<div id="<%= dom_id note %>">
  <p>
    <strong>Title:</strong>
    <%= note.title %>
  </p>

  <p>
    <strong>Content:</strong>
    <%= note.content %>
  </p>

  <p>
    <strong>File:</strong>
    <%= link_to note.file.filename, note.file if note.file.attached? %>
  </p>

  <p>
    <strong>User:</strong>
    <%= note.user_id %>
  </p>

</div>
